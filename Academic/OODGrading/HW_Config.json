{
    "Linux": {
        "_INTELLIJ_PATH" : ["/opt/idea/bin/idea"],
        "_PMD_PATH"      : "/opt/pmd-bin-7.10.0/bin/pmd",
        "_GRADLE_PATH"   : "/home/$USER/.sdkman/candidates/gradle/current/bin/gradle",
        "_EDITOR_COMMAND": "xed"
    },
    "Darwin": {
        "_INTELLIJ_PATH" : [
            "/Applications/IntelliJ\\ IDEA.app/Contents/MacOS/idea",
            "/Applications/IntelliJ\\ IDEA\\ CE.app/Contents/MacOS/idea"
        ],
        "_PMD_PATH"      : "/opt/pmd-bin-7.10.0/bin/pmd",
        "_GRADLE_PATH"   : "/home/$USER/.sdkman/candidates/gradle/current/bin/gradle",
        "_EDITOR_COMMAND": "open -e"
    },
    "Settings": {
        "_PMD_JAVA_RULES": "OOD_Java-Rules.xml",
        "_GET_RECENT"    : true,
        "_REPORT_DIR"    : "output",
        "_N_SEARCH_R"    : 8,
        "_RUN_TESTS"     : true,
        "_NUM_TESTS"     : 1,
        "_N_BIG_BLK"     : 25,
        "_INSPECT_J"     : true,
        "_SRCH_MARGN"    : 40,
        "_OPEN_SNPPT"    : true,
        "_EN_ALL_TST"    : true
    },
    "HWX": {
        "_LIST_PATHS": ["ugrads.txt","grads.txt"],
        "_SOURCE_DIR": "src/main/java/csci/ooad",
        "_TEST_DIR"  : "src/test/java/csci/ooad",
        "_BRANCH_STR": "6",
        "_TOPIC_SRCH": ["observer", "observor", "adaptor", "adapter", "facade", "singleton", "IMazeObservers", "AudibleObserver", 
                        "broadcast", "TestObserver", "IMazeSubject"]
    }
}
